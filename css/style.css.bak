@charset "UTF-8";
/*reset*/
*{padding:0;margin:0;}
ul, li, ol{list-style:none}
body{font-size:14px;color:#000;}
a{text-decoration:none;color: #000;}	
img{vertical-align: top;}
.clear:after{content:'';display:block;clear:both}

/*layout*/

#header {height:120px;}
#visual {width:1200px;height:360px;margin:0 auto}
#contents {width:1200px;margin:0 auto}


/*header*/
#header .inner{width:1200px;height:100%;margin: 0 auto;position:relative;background-color: #eee;}
#header .logo{position:absolute;left:1%;top:50%;margin-top:-15px;}
#header .topMenu{position:absolute;right:1%;top:3%}
#header .topMenu ul li{float:left}
#header .topMenu ul li a{font-size:0.8em;}
#header .topMenu ul li a:after{content:" | ";display: inline-block;padding:0 5px}
#header .topMenu ul li:last-child a:after{content:" ";}

#header .gnb{position:absolute;top:80px;width:100%;height:30px;background-color: #fff;}
#header .gnb > ul {width:600px;height:100%;margin:0 auto;margin-right:200px;}
#header .gnb > ul > li{float:left;width:150px;text-align:center;}
#header .gnb > ul > li > a{font:1.2em/30px 'Noto Sns KR Bold';}

#header .gnb .sub{width:1200px;height:360px;position:absolute;left:0;top:50px;z-index:30;}
#header .gnb .sub dl{width:18%;height:100%;float:left;text-align:left}
#header .gnb .sub dl:first-child{margin-left:120px;}
#header .gnb .sub dl dt{padding:10px 0;font:1.2em/30px 'Noto Sns KR';}
#header .gnb .sub dl dd {padding:8px 0;}
#header .gnb .sub dl > dd > ul{display:none}

#header .gnb_right {position:absolute;right:1%;top:80px;}
#header .gnb_right .login{height:30px;width:60px;font:bold 1.2em/20px 'Noto sns KR';border:0;background-color: #fff;}
#header .gnb_right .search{height:40px;width:40px;border:0;line-height:40px;background-color: #fff;}
#header .gnb_right .search img{width:100%;height:100%;}

#visual .v_slider{width:100%;height:100%;overflow:hidden;position:relative}
#visual .v_slider ul li {position:absolute;left:0;top:0;width:1200px;height:360px;display:inline-block;position:relative}
#visual .v_slider ul li a {display: block;width:100%;height:100%}
#visual .v_slider ul li a img:first-child{width:100%;height:100%}
#visual .v_slider ul li a img:last-child{width:300px;height:180px;position:absolute;top:100px;left:100px}

#contents .cont1{position:relative}
#contents .cont1 ul {width:780px;margin:0 auto;border-bottom:2px solid #eee;}
#contents .cont1 ul li{width:60px;float:left;height:100px;margin:60px 30px;background-color: #eee;position:relative;}
#contents .cont1 ul li:nth-child(7){clear:both}
#contents .cont1 ul li:nth-child(1),
#contents .cont1 ul li:nth-child(2),
#contents .cont1 ul li:nth-child(3),
#contents .cont1 ul li:nth-child(4),
#contents .cont1 ul li:nth-child(5),
#contents .cont1 ul li:nth-child(6){margin-bottom:0;}
		

#contents .cont1 ul li a{display:block;width:100%;height:100%;background:url(../img/cont1_icon.png) 0 0 no-repeat;background-size:800px 600px;}
#contents .cont1 ul li a:nth-child(1){background-position:-35px -20px}
#contents .cont1 ul li a span{position:absolute;left:0;bottom:10px}
#contents .cont1 > a{display: block;width:100px;height:30px;background-color: #eee;border-radius:50px;line-height:30px;text-align:center;position:absolute;left:50%;bottom:0%;margin-left:-50px;margin-bottom:-15px;}


#contents .cont2{padding-top:60px;height:250px}
#contents .cont2 .cont2_box1{width:50%;height:250px;background-color: #9999ff;float:left}
#contents .cont2 .cont2_box1 a{display: block;width:100%;height:100%}
#contents .cont2 .cont2_box1 .txt_box{width:60%;float:left}
#contents .cont2 .cont2_box1 .txt_box h3 {font-size:2em;color: #fff;padding-top:20%;text-indent:30px;}
#contents .cont2 .cont2_box1 .txt_box p{font-size:1.2em;color: #fff;padding-top:5%;text-indent:30px}
#contents .cont2 .cont2_box1 .icon_box{width:40%;height:100%;float:left;position:relative;}
#contents .cont2 .cont2_box1 .icon_box .icon_bc {width:80%;height:80%;border-radius:50%;background-color: #fff;position:absolute;top:50%;margin-top:-100px;}
#contents .cont2 .cont2_box1 .icon_box .icon_bc img{width:65%;height:65%;position:absolute;top:50%;left:50%;margin-top:-60px;margin-left:-60px;}

#contents .cont2 .cont2_box2{width:50%;height:250px;background-color: #9999ff;float:left}
#contents .cont2 .cont2_box2 a{display: block;width:100%;height:100%}
#contents .cont2 .cont2_box2 .txt_box{width:60%;float:left}
#contents .cont2 .cont2_box2 .txt_box h3 {font-size:2em;color: #fff;padding-top:20%;text-indent:30px;}
#contents .cont2 .cont2_box2 .txt_box p{font-size:1.2em;color: #fff;padding-top:5%;text-indent:30px}
#contents .cont2 .cont2_box2 .icon_box{width:40%;height:100%;float:left;position:relative;}
#contents .cont2 .cont2_box2 .icon_box .icon_bc {width:80%;height:80%;border-radius:50%;background-color: #fff;position:absolute;top:50%;margin-top:-100px;}
#contents .cont2 .cont2_box2 .icon_box .icon_bc img{width:65%;height:65%;position:absolute;top:50%;left:50%;margin-top:-60px;margin-left:-60px;}

#contents .cont3{width:100%;height:600px;}

#contents .cont3 .cont3_box1 {width:50%;float:left;height:100%;background-color: #6600ff;}
#contents .cont3 .cont3_box1 h3{padding-top:40px;text-indent:80px;}
#contents .cont3 .cont3_box1 h3 a{font-size:1.5em}
#contents .cont3 .cont3_box1 ul {width:90%;margin:0 auto}
#contents .cont3 .cont3_box1 ul li{width:100%;padding:3px 0;background-color: #eee;border-radius:20px;margin:20px 0;}
#contents .cont3 .cont3_box1 ul li a{display: block;width:97%;height:100%;margin-left:3%;}
#contents .cont3 .cont3_box1 ul li a span{line-height:50px;font-size:1.2em}

#contents .cont3 .cont3_box2 {width:50%;float:left;height:50%;	background-color: #ff3333}
#contents .cont3 .cont3_box2 h3{padding-top:40px;text-indent:80px;}
#contents .cont3 .cont3_box2 h3 a{font-size:1.5em}
#contents .cont3 .cont3_box2 ul {width:90%;height:180px;margin:0 auto;position:relative;border-radius:30px;overflow:hidden}
#contents .cont3 .cont3_box2 ul li{width:100%;height:100%;float:left;position:absolute;top:0;left:0}
#contents .cont3 .cont3_box2 ul li img {width:100%;height:100%}

#contents .cont3 .cont3_box3 {width:50%;;float:left;height:50%;background-color:#00cc00;position:relative}
#contents .cont3 .cont3_box3 h3{padding-top:40px;text-indent:80px;}
#contents .cont3 .cont3_box3 h3 a{font-size:1.5em}
#contents .cont3 .cont3_box3 ul {width:90%;height:180px;margin:0 auto;position:relative;}
#contents .cont3 .cont3_box3 ul li{width:100%;height:100%;float:left;position:absolute;top:0;left:0;border:2px solid #eee;border-radius:30px}
#contents .cont3 .cont3_box3 ul li .txt_box{width:55%;height:100%;float:left}
#contents .cont3 .cont3_box3 ul li .img_box{width:45%;height:100%;float:left;position:relative}
#contents .cont3 .cont3_box3 ul li h6{padding-top:40px;padding-left:70px;font-size:1.3em}
#contents .cont3 .cont3_box3 ul li p{padding-top:10px;padding-left:70px;font-size:1.2em;}
#contents .cont3 .cont3_box3 ul li img{width:50%;height:70%;position:absolute;top:50%;left:50%;margin-top:-65px;margin-left:-65px;}
#contents .cont3 .cont3_box3 .left {display: block;width:15px;height:24px;position:absolute;left:50px;top:60%;margin-top:-15px;}

#contents .cont3 .cont3_box3 .right {display: block;width:15px;height:24px;position:absolute;right:50px;top:60%;margin-top:-15px;}

#contents .cont4 {background-color: #99ccff;width:100%;height:450px;}
#contents .cont4 .cont4_box1{width:50%;float:left;margin-top:40px;}
#contents .cont4 .cont4_box1 .bc_box{width:90%;height:350px;margin:0 auto;background-color: #fff;border-radius:30px;overflow:hidden}
#contents .cont4 .cont4_box1 h3{padding-top:30px;text-indent:60px;}
#contents .cont4 .cont4_box1 h3 a{font-size:1.2em}
#contents .cont4 .cont4_box1 ul {width:90%;margin:0 auto;}
#contents .cont4 .cont4_box1 ul li {padding:10px 0;font-size:1em;font-weight:bold;}
#contents .cont4 .cont4_box1 li a span:first-child{color:#c7c7c7;}
#contents .cont4 .cont4_box1 li a span:last-child{margin-left:30px;color:#000;}


#contents .cont4 .cont4_box2 {width:50%;float:left;margin-top:40px;}
#contents .cont4 .cont4_box2 .bc_box{position:relative;width:90%;height:165px;margin:0 auto;background-color: #fff;border-radius:30px;overflow:hidden}
#contents .cont4 .cont4_box2 h3{padding-top:30px;text-indent:60px;}
#contents .cont4 .cont4_box2 h3 a{font-size:1.2em}
#contents .cont4 .cont4_box2 p {display: inline-block;margin-left:80px}
#contents .cont4 .cont4_box2 .bc_box .consult_icon{height:120px;width:100px;position:absolute;top:50%;right:10%;margin-top:-60px;}

#contents .cont4 .cont4_box3 {width:50%;float:left;margin-top:20px;}
#contents .cont4 .cont4_box3 .bc_box{position:relative;width:90%;height:165px;margin:0 auto;background-color: #fff;border-radius:30px;overflow:hidden}
#contents .cont4 .cont4_box3 h3{padding-top:30px;text-indent:60px;}
#contents .cont4 .cont4_box3 h3 a{font-size:1.2em}
#contents .cont4 .cont4_box3 ul{width:300px;margin-left:80px;}
#contents .cont4 .cont4_box3 ul li {padding:5px 0;width:49%;float:left}
#contents .cont4 .cont4_box3 .service_icon{width:100px;height:120px;position:absolute;top:50%;right:10%;margin-top: -50px;}

#footer .inner{width:1200px;margin:0 auto;}
#footer .inner .f1{width:100%;height:80px;background-color: #aaa;float:left}

#footer .inner .f1 ul li{width:20%;height:100%;float:left}
#footer .inner .f1 ul li a{display: block;width:100%;height:100%;text-align:center;line-height:80px;font-size:1.2em;color:#eee}
#footer .inner .f1 ul li a:after{content:"¡å";display: inline-block;}

#footer .inner .f2{width:100%;height:200px;background-color: #eee;float:left}
#footer .inner .f2 .f2_box1 {width:100%;float:left;padding-top:30px}
#footer .inner .f2 .f2_box1 ul li{float:left;padding:8px 0}
#footer .inner .f2 .f2_box1 ul:last-child li:first-child{margin-left:17%;}
#footer .inner .f2 .f2_box1 ul li:first-child {margin-left:23%;}
#footer .inner .f2 .f2_box1 ul li a:after {content:'|';display:inline-block;padding:0 3px}
#footer .inner .f2 .f2_box1 ul li:last-child a:after {content:''}

#footer .inner .f2 .f2_box2 {width:80%;float:left;padding-top:10px;}
#footer .inner .f2 .f2_box2 p{text-align:left;padding:8px 0;text-indent:150px;}

#footer .inner .f2 .f2_box3 {width:20%;float:left;padding-top:20px}	
#footer .inner .f2 .f2_box3 ul li {float:left;padding:5px}

